#include <stdio.h>
#include <stdlib.h>
#include <string.h>




int ***vetor(int n,int m,int k){
    int ***a;
    a = malloc(n * sizeof(int**));
    for(int i = 0;i < n;i++){
        a[i] = malloc(m * sizeof(int*));
        for(int j = 0;j < m;j++){
            a[i][j] = malloc(k * sizeof(int));
        }
    }
    return a;
}

